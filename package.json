{
  "name": "antd-graphql-table",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --emitDeclarationOnly && babel src --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"stories/**/*.ts\"",
    "lint": "eslint \"{src,stories}/**/*.{js,jsx,ts,tsx}\" --fix",
    "prepublish": "npm run build",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowolfapp/antd-graphql-table.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowolfapp/antd-graphql-table/issues"
  },
  "homepage": "https://github.com/shadowolfapp/antd-graphql-table#readme",
  "dependencies": {
    "antd": "^4.2.4",
    "antd-simple-table": "^1.0.5",
    "keyboardjs": "^2.5.1",
    "qs": "^6.9.4",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/keyboardjs": "^2.5.0",
    "@types/lodash": "^4.14.152",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.34",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.1.2",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-standard-with-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "semantic-release": "^17.0.7",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{json,js,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
