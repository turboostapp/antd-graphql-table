{
  "name": "antd-graphql-table",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --emitDeclarationOnly && babel src --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"stories/**/*.ts\"",
    "prepublish": "npm run build",
    "storybook": "start-storybook",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "build-storybook": "build-storybook -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowolfapp/antd-graphql-table.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowolfapp/antd-graphql-table/issues"
  },
  "homepage": "https://github.com/shadowolfapp/antd-graphql-table#readme",
  "dependencies": {
    "antd": "^4.5.3",
    "antd-simple-table": "^1.4.11",
    "keyboardjs": "^2.6.3",
    "qs": "^6.9.4",
    "react-router-dom": "^5.2.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@superboost/browserslist-config": "^1.0.0",
    "@types/keyboardjs": "^2.5.0",
    "@types/lodash": "^4.14.155",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.37",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.1.2",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "lint-staged": "^10.2.10",
    "lodash": "^4.17.15",
    "markdown-loader": "^5.1.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "semantic-release": "^19.0.3",
    "storybook-readme": "^5.0.8",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "extends @superboost/browserslist-config"
  ]
}
